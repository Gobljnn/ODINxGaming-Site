@model IEnumerable<OdinXSiteMVC2.Models.Socials.ExecSocial>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Socials</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>



@foreach (var item in Model) {

<div class="card backdrop">
  <img src="@item.scLink" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title">@Html.DisplayFor(modelItem => item.execName)</h5>
    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
  </div>
  <ul class="list-group list-group-flush ">
    <li class="list-group-item backdrop">An item</li>
    <li class="list-group-item backdrop">A second item</li>
    <li class="list-group-item backdrop">A third item</li>
  </ul>
  <div class="card-body">
    <a href="#" class="card-link">Card link</a>
    <a href="#" class="card-link">Another link</a>
  </div>
</div>

    }

